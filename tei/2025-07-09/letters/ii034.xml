   <text>
      <body divRole="original-translation">
         <div type="original" xml:lang="nl">
            <pb xml:id="pb.orig.nl.1" f="1r" facs="#s1r" n="1"/>
            <p xml:id="p.orig.nl.1" n="1">Ruiken zeer lekker, merci. Pleizierige S<hi rend="super">t</hi> Niklaas.<ptr type="note" target="#note.orig.nl.1"/>
            </p>
            <space dim="vertical" unit="lines" quantity="1"/>
            <p xml:id="p.orig.nl.2" n="2">Adieu.</p>
            <space dim="vertical" unit="lines" quantity="1"/>
            <pb xml:id="pb.orig.nl.2" f="1v" facs="#s1v" n="2"/>
            <p xml:id="p.orig.nl.3" n="3">Mevr. v. gogh</p>
            <p xml:id="p.orig.nl.4" n="4">Villa Helma</p>
            <p xml:id="p.orig.nl.5" n="5">Bussum.</p>
         </div>
         <div type="translation" xml:lang="en">
            <pb xml:id="pb.tr.en.1"
                corresp="#pb.orig.nl.1"
                f="1r"
                facs="#s1r"
                n="1"/>
            <p xml:id="p.tr.en.1" corresp="#p.orig.nl.1" n="1">They smell very good, thank you. Happy St Nicholas.<ptr type="note" target="#note.tr.en.1"/>
            </p>
            <space dim="vertical" unit="lines" quantity="1"/>
            <p xml:id="p.tr.en.2" corresp="#p.orig.nl.2" n="2">Adieu.</p>
            <space dim="vertical" unit="lines" quantity="1"/>
            <pb xml:id="pb.tr.en.2"
                corresp="#pb.orig.nl.2"
                f="1v"
                facs="#s1v"
                n="2"/>
            <p xml:id="p.tr.en.3" corresp="#p.orig.nl.3" n="3">Mevr. v. gogh</p>
            <p xml:id="p.tr.en.4" corresp="#p.orig.nl.4" n="4">Villa Helma</p>
            <p xml:id="p.tr.en.5" corresp="#p.orig.nl.5" n="5">Bussum.</p>
         </div>
      </body>
   </text>
   <standOff>
      <listAnnotation type="typednotes" xml:lang="nl">
